}
#sleepophie {
  height: 550px;
  width: 800px;
}
#terv {
  height: 450px;
  width: 650px;
}
#outophie {
  height: 600px;
  width: 800px;
}
#pom {
  height: 450px;
  width: 650px;
}
#zoomophie {
  height: 550px;
  width: 800px;
}
#gnophie {
  height: 650px;
  width: 550px;
}
#shelt {
  height: 450px;
