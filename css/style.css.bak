#wrapper {
  width: 850px;
  height: 600px;
  background-color: #cc0;
  margin:0 auto;
  overflow: hidden;
}
#canvas {
  width: 700px;
  height: 500px;
  background-color: #ccc;
  margin: 30px auto;
}
#myMap {
  height: 500px;
  width: 100%;
}
#subtitle {
  text-align:center;
  margin-left: auto;
  margin-right: auto;
}
#slider {
  margin:0 auto;
}
@media screen and (min-width: 1024px) and (orientation: landscape) {
  #flexbox {
    display: flex;
    flex-direction: row;
    flex-wrap:wrap;
    justify-content: space-between;
  }
  
  #pagewrapper {
    width: 60%;
    margin: 0 auto;
    max-width: 960px;
  }
}

@media screen and (min-width: 768px) and (max-width: 1024px) and (orientation: landscape) {
  #flexbox {
    display: flex;
    flex-direction: row;
    flex-wrap:wrap;
    justify-content: space-between;
  }

  #pagewrapper {
    width: 80%;
    margin: 0 auto;
    max-width: 960px;
  }
}